            <string>///MUSIC PLAYER

// Music Player Flags
start_1 = track_pointer == 6 &amp;&amp; !audio_is_playing(music6_livinginthedark) &amp;&amp; !audio_is_playing(music1_angst);
start_2 = track_pointer == 1 &amp;&amp; !audio_is_playing(music1_angst) &amp;&amp; !audio_is_playing(music2_coldshivers);
start_3 = track_pointer == 2 &amp;&amp; !audio_is_playing(music2_coldshivers) &amp;&amp; !audio_is_playing(music3_danger);
start_4 = track_pointer == 3 &amp;&amp; !audio_is_playing(music3_danger) &amp;&amp; !audio_is_playing(music4_ff);
start_5 = track_pointer == 4 &amp;&amp; !audio_is_playing(music4_ff) &amp;&amp; !audio_is_playing(music5_forsaken);
start_6 = track_pointer == 5 &amp;&amp; !audio_is_playing(music5_forsaken) &amp;&amp; !audio_is_playing(music6_livinginthedark);
 
// If sound_track 4 was played and is not currently playing and sound track 1 is not already playing
if(start_1)
{
    audio_play_sound(music1_angst, 1, false);
 
    // Set track_pointer to 1 as sound_track_1 has been played
    track_pointer = 1;
}
 
if(start_2){
 
    audio_play_sound(music2_coldshivers, 1, false);
 
    track_pointer = 2;
} 
 
if(start_3){
 
    audio_play_sound(music3_danger, 1, false);
 
    track_pointer = 3;
}  
 
if(start_4){
 
    audio_play_sound(music4_ff, 1, false);
 
    track_pointer = 4;
}

if(start_5){
 
    audio_play_sound(music5_forsaken, 1, false);
 
    track_pointer = 5;
}

if(start_6){
 
    audio_play_sound(music6_livinginthedark, 1, false);
 
    track_pointer = 6;
}
</string>
